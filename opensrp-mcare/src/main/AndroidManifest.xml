<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.ei.opensrp.mcare">

    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NativeHomeActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".household.HouseHoldSmartRegisterActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".household.tutorial.tutorialCircleViewFlow"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".household.HouseHoldDetailActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".elco.ElcoSmartRegisterActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".elco.ElcoDetailActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />

    </application>

</manifest>
